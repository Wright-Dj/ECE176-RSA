module Fast_Modular_Exponentiator_tb();
  
  parameter base_N = 556;
  parameter modulus_N = 556;
  parameter exponent_N = 556;
  
  wire [modulus_N-1:0] out;
  reg [base_N-1:0] base;
  reg [modulus_N-1:0] modulus;
  reg [exponent_N-1:0] exponent;
  reg st, clk, reset;
  
  Fast_Modular_Exponentiator t2(.out, .base, .modulus, .exponent, .st, .clk, .reset);
  
  // Using a Testcases from...
  // https://en.wikipedia.org/wiki/RSA_(cryptosystem)#Example
  //https://www.di-mgt.com.au/rsa_alg.html
  initial begin
    base=65;
    modulus=3233;
    exponent=17;
    clk=0;
    st=0;
    reset=0;
    #60
    st=1;
    //Expected out= 2790
    #40
    st=0;
    #60
    st=1;
    base=16187; 
    exponent=3;
    modulus =25777; 
    // Expected out= 24465
    #60
    reset=1;
    #60
    base =18524;
    exponent=16971;
    modulus = 25777;
    reset=0;
    // Expected out = 1289
   
  end
  
  always #10 clk=~clk;
  
  always @(out)
    $display("Time: ", $time, ", Output =%d", out);
  
endmodule